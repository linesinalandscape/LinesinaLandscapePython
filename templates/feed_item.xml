<entry>
<title type="html">{{ title }}</title>
<link href="{{ permalink }}" rel="alternate" type="text/html" title="{{ title }}" />
<published>{{ date_feed }}</published>
<updated>{{ date_feed_updated }}</updated>
<id>{{ permalink }}</id>
<content type="html">
<![CDATA[{{ content }}]]>
</content>
<author><name>{{ site_author }}</name></author>
<summary type="html">
<![CDATA[{{ description }}]]>
</summary>
</entry>
